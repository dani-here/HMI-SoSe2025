{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=LLMWrapperDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "LatinSquareSequences": [
    [ "Labeling", "Creative", "Analytical", "Procedural" ],
    [ "Creative", "Analytical", "Procedural", "Labeling" ],
    [ "Analytical", "Procedural", "Labeling", "Creative" ],
    [ "Procedural", "Labeling", "Creative", "Analytical" ]
  ],
  "LLMConfig": {
    // not being utilized, but kept for future reference
    "TopP": 1.0,
    "Model": "gpt-4o",
    "MaxTokens": 4096,
    "Temperature": 0.7,
    "PresencePenalty": 0.0,
    "FrequencyPenalty": 0.0,
    "StopSequences": [ "\n" ],

    "MinResponseMs": 2000, // 2 seconds minimum response time
    "DefaultModel": "gpt-4o",
    "MaxAllowedParticipants": 100,
    "OpenAIBaseUrl": "https://api.openai.com/v1/chat/completions",
    "OpenAPIKey": "api-key-here",
    "MasterPrompt": "You are an AI assistant in a research study about prompting strategies. Always answer clearly and concisely, but do not over-assume user intent. Stay neutral, and let the user guide the conversation. If a question is ambiguous, ask clarifying questions instead of guessing."
  },
  "AllowedHosts": "*"
}
